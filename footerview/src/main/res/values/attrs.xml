<?xml version="1.0" encoding="utf-8"?>
<resources>


    <declare-styleable name="FooterViewLP">
        <attr name="layout_isPined" format="boolean" />
        <attr name="layout_isHandler" format="boolean" />
    </declare-styleable>
    <declare-styleable name="FooterView">
        <attr name="shadowBehind" format="boolean" />
        <attr name="touchableHandler" format="boolean" />
        <attr name="closable" format="boolean" />
        <attr name="cornerTopRight" format="dimension" />
        <attr name="cornerTopLeft" format="dimension" />
        <attr name="cornerBottomRight" format="dimension" />
        <attr name="cornerBottomLeft" format="dimension" />
        <attr name="backgroundColorCard" format="color" />

        <attr name="cardElevation" format="dimension" />
        <attr name="startType" format="enum">
            <enum name="close" value="0" />
            <enum name="open" value="1" />
            <enum name="openFull" value="2" />
        </attr>
    </declare-styleable>
</resources>